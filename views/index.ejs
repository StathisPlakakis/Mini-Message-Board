<html>
  <head>
    <title>Message-App</title>
    <link rel="stylesheet" href="style.css">
  </head>
  <body>
    <% messages.forEach((message) => { %>
      <div>
        <h1><%= message.user %></h1>
        <p><%= message.text %></p>
        <p><%= message.added %></p>
      </div>
    <% }); %>
  </body>
</html>